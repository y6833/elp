{
  "name": "frontend-engineering-playground",
  "version": "1.0.0",
  "description": "一个通过关卡式学习掌握前端工程化（33个关卡）的交互式项目",
  "main": "index.js",
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "build": "node scripts/build.js",
    "start": "node start.js",
    "server": "node server/index.js",
    "setup:levels": "node scripts/setup-levels.js",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:e2e": "cypress run",
    "test:coverage": "jest --coverage",
    "build:all": "npm run build:webpack && npm run build:vite && npm run build:rollup",
    "build:rollup": "rollup -c",
    "build:parcel": "parcel build src/index.html",
    "build:esbuild": "node build.js",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"**/*.{js,ts,jsx,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,jsx,tsx,json,md}\"",
    "clean": "rimraf dist build .cache .parcel-cache coverage"
  },
  "keywords": [
    "webpack",
    "vite",
    "rollup",
    "parcel",
    "esbuild",
    "swc",
    "frontend",
    "engineering",
    "learning",
    "yarn",
    "pnpm",
    "monorepo",
    "jest",
    "cypress",
    "playwright",
    "github-actions",
    "gitlab-ci",
    "docker",
    "pwa",
    "micro-frontends",
    "performance",
    "netlify",
    "vercel",
    "serverless",
    "kubernetes"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "express": "^4.18.2",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "vite": "^4.4.0",
    "html-webpack-plugin": "^5.5.3",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "file-loader": "^6.2.0",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "inquirer": "^8.2.6"
  }
}
