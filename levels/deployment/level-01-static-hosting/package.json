{
  "name": "static-hosting-demo",
  "version": "1.0.0",
  "description": "静态网站托管示例项目",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=dist",
    "deploy:vercel": "npm run build && vercel --prod",
    "deploy:gh-pages": "npm run build && gh-pages -d dist"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.1.0",
    "vite": "^4.1.0",
    "gh-pages": "^5.0.0",
    "netlify-cli": "^13.0.0",
    "vercel": "^28.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}